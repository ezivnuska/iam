#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/home/Projects/iam/node_modules/.pnpm/webpack-cli@6.0.1_webpack-bundle-analyzer@4.10.2_webpack-dev-server@5.2.1_webpack@5.99.6/node_modules/webpack-cli/bin/node_modules:/Users/home/Projects/iam/node_modules/.pnpm/webpack-cli@6.0.1_webpack-bundle-analyzer@4.10.2_webpack-dev-server@5.2.1_webpack@5.99.6/node_modules/webpack-cli/node_modules:/Users/home/Projects/iam/node_modules/.pnpm/webpack-cli@6.0.1_webpack-bundle-analyzer@4.10.2_webpack-dev-server@5.2.1_webpack@5.99.6/node_modules:/Users/home/Projects/iam/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/home/Projects/iam/node_modules/.pnpm/webpack-cli@6.0.1_webpack-bundle-analyzer@4.10.2_webpack-dev-server@5.2.1_webpack@5.99.6/node_modules/webpack-cli/bin/node_modules:/Users/home/Projects/iam/node_modules/.pnpm/webpack-cli@6.0.1_webpack-bundle-analyzer@4.10.2_webpack-dev-server@5.2.1_webpack@5.99.6/node_modules/webpack-cli/node_modules:/Users/home/Projects/iam/node_modules/.pnpm/webpack-cli@6.0.1_webpack-bundle-analyzer@4.10.2_webpack-dev-server@5.2.1_webpack@5.99.6/node_modules:/Users/home/Projects/iam/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack-cli/bin/cli.js" "$@"
else
  exec node  "$basedir/../webpack-cli/bin/cli.js" "$@"
fi
