#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/home/Projects/iam/node_modules/.pnpm/expo@52.0.44_@babel+core@7.26.10_@babel+preset-env@7.26.9_@babel+core@7.26.10__react-na_36acd2b5e5986286f838583fe3f80c3a/node_modules/expo/bin/node_modules:/Users/home/Projects/iam/node_modules/.pnpm/expo@52.0.44_@babel+core@7.26.10_@babel+preset-env@7.26.9_@babel+core@7.26.10__react-na_36acd2b5e5986286f838583fe3f80c3a/node_modules/expo/node_modules:/Users/home/Projects/iam/node_modules/.pnpm/expo@52.0.44_@babel+core@7.26.10_@babel+preset-env@7.26.9_@babel+core@7.26.10__react-na_36acd2b5e5986286f838583fe3f80c3a/node_modules:/Users/home/Projects/iam/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/home/Projects/iam/node_modules/.pnpm/expo@52.0.44_@babel+core@7.26.10_@babel+preset-env@7.26.9_@babel+core@7.26.10__react-na_36acd2b5e5986286f838583fe3f80c3a/node_modules/expo/bin/node_modules:/Users/home/Projects/iam/node_modules/.pnpm/expo@52.0.44_@babel+core@7.26.10_@babel+preset-env@7.26.9_@babel+core@7.26.10__react-na_36acd2b5e5986286f838583fe3f80c3a/node_modules/expo/node_modules:/Users/home/Projects/iam/node_modules/.pnpm/expo@52.0.44_@babel+core@7.26.10_@babel+preset-env@7.26.9_@babel+core@7.26.10__react-na_36acd2b5e5986286f838583fe3f80c3a/node_modules:/Users/home/Projects/iam/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../expo/bin/cli" "$@"
else
  exec node  "$basedir/../expo/bin/cli" "$@"
fi
