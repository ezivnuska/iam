#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/home/Projects/iam/node_modules/.pnpm/parcel@2.14.4_@swc+helpers@0.5.17_postcss@8.4.49_relateurl@0.2.7_terser@5.39.0_typescript@4.9.5/node_modules/parcel/lib/node_modules:/Users/home/Projects/iam/node_modules/.pnpm/parcel@2.14.4_@swc+helpers@0.5.17_postcss@8.4.49_relateurl@0.2.7_terser@5.39.0_typescript@4.9.5/node_modules/parcel/node_modules:/Users/home/Projects/iam/node_modules/.pnpm/parcel@2.14.4_@swc+helpers@0.5.17_postcss@8.4.49_relateurl@0.2.7_terser@5.39.0_typescript@4.9.5/node_modules:/Users/home/Projects/iam/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/home/Projects/iam/node_modules/.pnpm/parcel@2.14.4_@swc+helpers@0.5.17_postcss@8.4.49_relateurl@0.2.7_terser@5.39.0_typescript@4.9.5/node_modules/parcel/lib/node_modules:/Users/home/Projects/iam/node_modules/.pnpm/parcel@2.14.4_@swc+helpers@0.5.17_postcss@8.4.49_relateurl@0.2.7_terser@5.39.0_typescript@4.9.5/node_modules/parcel/node_modules:/Users/home/Projects/iam/node_modules/.pnpm/parcel@2.14.4_@swc+helpers@0.5.17_postcss@8.4.49_relateurl@0.2.7_terser@5.39.0_typescript@4.9.5/node_modules:/Users/home/Projects/iam/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../parcel/lib/bin.js" "$@"
else
  exec node  "$basedir/../parcel/lib/bin.js" "$@"
fi
